{
  "name": "nest-typescript-starter",
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "env:cmd": "pnpm env:setup && pnpm env:local",
    "env:common": "env-cmd -f .env.common env-cmd -f .env.local.me",
    "env:local": "pnpm env:common env-cmd -f .env.local env-cmd -f .env.local.me",
    "env:int": "pnpm env:local env-cmd -f .env.int env-cmd -f .env.int.local",
    "env:ci": "pnpm env:common env-cmd -f .env.int  env-cmd -f .env.ci env-cmd -f .env.local.me",
    "env:setup": "[ ! -f .env.local.me ] && awk '/#DELIMITER=#/{flag=1; next} flag' .env.local > .env.local.me || true",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "prisma:rebuild": "npm prisma:generate && npm prisma:push:reset && npm prisma:seed:test",
    "prisma:setup": "npm prisma:generate && npm prisma:seed:test",
    "prisma:generate": "DEBUG='*' npm run prisma:merge && npm run prisma:client",
    "prisma:merge": "prisma-import --force --schemas './src/modules/**/*.prisma' --output './src/generated/schema.prisma'",
    "prisma:client": "prisma generate --schema ./src/generated/schema.prisma",
    "prisma:reset": "npm env:cmd npm prisma migrate reset --schema ./src/generated/schema.prisma --force",
    "prisma:migrate": "npm env:cmd prisma migrate dev --schema ./src/generated/schema.prisma",
    "prisma:migrate:int": "npm env:int prisma migrate dev --schema ./src/generated/schema.prisma",
    "prisma:migrate:ci": "npm env:ci prisma migrate dev --schema ./src/generated/schema.prisma",
    "prisma:migrate:docker": "npm prisma migrate dev --schema ./src/generated/schema.prisma",
    "prisma:push": "npm env:cmd prisma db push --schema ./src/generated/schema.prisma",
    "prisma:push:reset": "npm env:cmd prisma db push --force-reset --schema ./src/generated/schema.prisma",
    "prisma:deploy": "npm env:cmd prisma migrate deploy --schema ./src/generated/schema.prisma",
    "prisma:deploy:production": "npm prisma migrate deploy --schema ./src/generated/schema.prisma",
    "prisma:seed": "npm env:cmd tsx src/modules/prisma/seed/index.ts",
    "prisma:seed:test": "npm prisma:seed && npm env:cmd tsx src/testing/seed/index.ts",
    "prisma:studio": "npm env:cmd prisma studio --schema ./src/generated/schema.prisma",
    "start": "pnpm env:cmd nest start",
    "start:dev": "pnpm env:cmd nest start --watch",
    "start:e2e": "./docker/init_database.sh && env-cmd -f .env.e2e pnpm nest start",
    "start:e2e:ci": "env-cmd -f .env.e2e pnpm nodemon -r tsconfig-paths/register src/main.ts --run-in-band",
    "start:debug": "nest start --tsc --debug 0.0.0.0:9229 --watch",
    "start:dist": "pnpm env:local node dist/src/main.js",
    "start:prod": "node dist/src/main.js",
    "lint": "eslint '{src,apps,libs,test}/**/*.ts' --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "echo 'No e2e tests implemented yet.'"
  },
  "dependencies": {
    "@nestjs/common": "10.0.3",
    "@nestjs/core": "10.0.3",
    "@nestjs/event-emitter": "^2.0.3",
    "@nestjs/platform-express": "10.0.3",
    "@nestjs/swagger": "7.0.4",
    "@prisma/client": "^5.6.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "env-cmd": "^10.1.0",
    "fastify": "^4.25.2",
    "nestjs-pino": "^3.5.0",
    "nodemon": "^3.0.2",
    "reflect-metadata": "0.1.13",
    "rimraf": "5.0.1",
    "rxjs": "7.8.1"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@nestjs/cli": "10.0.5",
    "@nestjs/schematics": "10.0.1",
    "@nestjs/testing": "10.0.3",
    "@types/express": "4.17.18",
    "@types/jest": "^29.5.11",
    "@types/node": "20.3.3",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.60.1",
    "@typescript-eslint/parser": "5.60.1",
    "eslint": "8.42.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.27.5",
    "jest": "^29.6.4",
    "jest-junit": "^16.0.0",
    "jest-mock-extended": "^3.0.5",
    "prettier": "2.8.8",
    "prisma": "^5.7.1",
    "prisma-erd-generator": "^1.11.2",
    "prisma-import": "^1.0.4",
    "prisma-json-types-generator": "^3.0.3",
    "supertest": "6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "9.4.4",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.1.6"
  }
}
