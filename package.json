{
  "name": "nest-typescript-starter",
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "env:cmd": "npm run env:setup && npm run env:local",
    "env:local": " dotenv -c -e .env.local.me -e .env.local --",
    "env:setup": "[ ! -f .env.local.me ] && awk '/#DELIMITER=#/{flag=1; next} flag' .env.local > .env.local.me || true",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint '{src,apps,libs,test}/**/*.ts' --fix",
    "prisma:rebuild": "npm run prisma:generate && npm run prisma:push:reset && npm run prisma:seed:test",
    "prisma:setup": "npm run prisma:generate && npm run prisma:seed:test",
    "prisma:generate": "DEBUG='*' npm run prisma:merge && npm run prisma:client",
    "prisma:merge": "prisma-import --force --schemas './src/modules/**/*.prisma' --output './src/generated/schema.prisma'",
    "prisma:client": "prisma generate --schema ./src/generated/schema.prisma",
    "prisma:reset": "npx env:cmd npm run prisma migrate reset --schema ./src/generated/schema.prisma --force",
    "prisma:migrate": "npm run env:local -- npx prisma migrate dev --schema ./src/generated/schema.prisma",
    "prisma:migrate:docker": "npm run prisma migrate dev --schema ./src/generated/schema.prisma",
    "prisma:push": "npm run env:local -- npx prisma db push --schema ./src/generated/schema.prisma",
    "prisma:push:reset": "npm run env:local -- npx prisma db push --force-reset --schema ./src/generated/schema.prisma",
    "prisma:deploy": "npm run env:local -- npx prisma migrate deploy --schema ./src/generated/schema.prisma",
    "prisma:deploy:production": "npx prisma migrate deploy --schema ./src/generated/schema.prisma",
    "prisma:seed": "npm run env:local -- npx tsx src/modules/prisma/seed/index.ts",
    "prisma:seed:test": "npm run prisma:seed && npx env:cmd tsx src/testing/seed/index.ts",
    "prisma:studio": "npm run env:local -- npx prisma studio --schema ./src/generated/schema.prisma",
    "setup:db": "npm run env:cmd docker-compose down api-core-db && docker-compose up api-core-db -d",
    "start": "npx env:cmd nest start",
    "start:dev": "npm run env:local -- nest start --watch",
    "start:e2e": "./docker/init_database.sh && dotenv -e .env.e2e pnpm nest start",
    "start:debug": "npm run env:local -- nest start --tsc --debug 0.0.0.0:9229 --watch",
    "start:dist": "npm run env:local -- node dist/src/main.js",
    "start:prod": "node dist/src/main.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "echo 'No e2e tests implemented yet.'"
  },
  "dependencies": {
    "@faker-js/faker": "^8.3.1",
    "@nestjs/common": "10.0.3",
    "@nestjs/core": "10.0.3",
    "@nestjs/event-emitter": "^2.0.3",
    "@nestjs/platform-express": "10.0.3",
    "@nestjs/swagger": "7.0.4",
    "@prisma/client": "^5.6.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "dotenv-cli": "^7.3.0",
    "faker": "^6.6.6",
    "faker-js": "^1.0.0",
    "fastify": "^4.25.2",
    "nestjs-pino": "^3.5.0",
    "nodemon": "^3.0.2",
    "pino-pretty": "^10.3.1",
    "reflect-metadata": "0.1.13",
    "rimraf": "5.0.1",
    "rxjs": "7.8.1"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@nestjs/cli": "10.0.5",
    "@nestjs/schematics": "10.0.1",
    "@nestjs/testing": "10.0.3",
    "@types/express": "4.17.18",
    "@types/jest": "^29.5.11",
    "@types/node": "20.3.3",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.60.1",
    "@typescript-eslint/parser": "5.60.1",
    "eslint": "8.42.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.27.5",
    "jest": "^29.6.4",
    "jest-junit": "^16.0.0",
    "jest-mock-extended": "^3.0.5",
    "prettier": "2.8.8",
    "prisma": "^5.7.1",
    "prisma-erd-generator": "^1.11.2",
    "prisma-import": "^1.0.4",
    "prisma-json-types-generator": "^3.0.3",
    "supertest": "6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "9.4.4",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.1.6"
  }
}
